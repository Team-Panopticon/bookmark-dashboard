{
  "name": "Bookmark Dashboard",
  "description": "Manage your Bookmark",
  "version": "2.0.2",
  "manifest_version": 3,
  "permissions": ["bookmarks"],
  "action": {
    "default_title": "Bookmark Dashboard",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "images/bookmark_dashboard_icon_16.png",
    "48": "images/bookmark_dashboard_icon_48.png",
    "128": "images/bookmark_dashboard_icon_128.png"
  }
}
